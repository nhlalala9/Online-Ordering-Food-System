{
  "kind": "collectionType",
  "collectionName": "order_lists",
  "info": {
    "singularName": "order-list",
    "pluralName": "order-lists",
    "displayName": "OrderList",
    "description": ""
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {},
  "attributes": {
    "Picture": {
      "type": "text"
    },
    "Price": {
      "type": "decimal"
    },
    "Date": {
      "type": "datetime"
    },
    "CustomerName": {
      "type": "text"
    },
    "Address": {
      "type": "text"
    },
    "Quantity": {
      "type": "integer"
    },
    "PaymentMethod": {
      "type": "enumeration",
      "enum": [
        "Card Payment",
        "EFT",
        "Cash",
        "Voucher"
      ]
    },
    "TotalPrice": {
      "type": "integer"
    },
    "products": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::product.product",
      "inversedBy": "order_lists"
    },
    "image": {
      "allowedTypes": [
        "images"
      ],
      "type": "media",
      "multiple": false
