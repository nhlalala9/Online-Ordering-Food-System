{
  "kind": "collectionType",
  "collectionName": "products",
  "info": {
    "singularName": "product",
    "pluralName": "products",
    "displayName": "Product",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string"
    },
    "price": {
      "type": "decimal"
    },
    "description": {
      "type": "text"
    },
    "Picture": {
      "type": "text"
    },
    "quantity": {
      "type": "integer"
    },
    "Total_Price": {
      "type": "integer"
    },
    "order_lists": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::order-list.order-list",
      "mappedBy": "products"
    },
    "image": {
      "allowedTypes": [
        "images"
      ],
      "type": "media",
      "multiple": false
    }
